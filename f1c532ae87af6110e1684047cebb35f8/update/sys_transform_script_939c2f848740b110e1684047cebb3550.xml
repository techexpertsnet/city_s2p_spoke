        <script><![CDATA[(function runTransformScript(source, map, log, target /*undefined onStart*/ ) {
	var attributes = source.getValue("u_attributes");
	var sol_details = JSON.parse(attributes);
	var src_email = sol_details['email'];	
	if(src_email != '') {
		//Ignore the import if the User with that email doesn't exist
		var usr = new GlideRecordSecure('sys_user');
		usr.addQuery('email',src_email);
		usr.query();
		if(!usr.next()){			
			ignore = true;			
		}
	}
})(source, map, log, target);]]></script>

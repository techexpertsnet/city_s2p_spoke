    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Hide Create CI Project button if Project ID contains a value</description>
        <field>x_citin_s2p_spoke_ci_project_id</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>City - Hide Create CI Project button</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   var ciProjectID = g_form.getValue('x_citin_s2p_spoke_ci_project_id');

   if (ciProjectID !== '') {
      var uiAction = new GlideUIAction('Create CI Project');
      uiAction.setVisible(false);
   }
}

// Run the script on form load
g_form.addInfoMessage('Running client script on form load');
onChange(null, null, null, true);

]]></script>
